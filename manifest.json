{
  "name": "Video Rotator",
  "description" : "Save your neck, and rotate the video not your head.",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "icon-16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/vidRot.css"],
      "js": ["js/jquery.js", "js/vidRot.js"]
    }
  ]
}